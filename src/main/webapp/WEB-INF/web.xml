<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>SignInServlet</servlet-name>
        <servlet-class>com.webapp.servlet.SignInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>com.webapp.servlet.SignUpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchMemberServlet</servlet-name>
        <servlet-class>com.webapp.servlet.SearchMemberServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchJobServlet</servlet-name>
        <servlet-class>com.webapp.servlet.SearchJobServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignInServlet</servlet-name>
        <url-pattern>/sign_in</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/sign_up</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchMemberServlet</servlet-name>
        <url-pattern>/member_search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchJobServlet</servlet-name>
        <url-pattern>/job_search</url-pattern>
    </servlet-mapping>
</web-app>
